#std::io::puts
#std::str::{fmt, show}

Rect := {
	width: int
	height: int
	color: str
}

rect_new: int int -> Rect
rect_new width height =
	Rect {
		width,
		height,
		"red"
	}

rect_show: Rect -> str
rect_show rect =
	fmt "{d}x{d} rect with color {s}"
		rect.width rect.height rect.color

main args =
	my_rect = rect_new 10 5
	->
		puts "my rect: ",
		-- prints "10x5 rect with color red"
		puts rect_show my_rect,

		0