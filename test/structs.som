#std::io::puts
#std::str::{fmt, show, Str}

Rect = {
	width: Int
	height: Int
	color: Str
}.

rect_new: Int -> Int -> Rect
rect_new width height =
	Rect {
		width,
		height,
		"red"
	}.

rect_show: Rect -> Str
rect_show rect =
	fmt "{}x{} rect with color {}" rect.width rect.height rect.color.

main args =
	my_rect = rect_new 10 5
	=>
		puts "my rect: ",
		-- prints "10x5 rect with color red"
		puts rect_show my_rect,

		0.